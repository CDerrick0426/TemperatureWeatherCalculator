//Clothing suggestion general outline
 if temperature >= 60
 {
    coat = null;
    bottoms = shorts;
    tops = tshirt;
 }
 if temperature < 60 && temperature > 32
 {
    coat = lightCoat;
    bottoms = pants;
    tops = tshirt;
 }
 else
 {
    coat = heavyCoat;
    bottoms = pants;
    tops = sweatshirt;
 }
